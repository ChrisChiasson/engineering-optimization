<?xml version="1.0" encoding="UTF-8"?>
<glossary xml:id="Glossary"
	xmlns="http://docbook.org/ns/docbook"
	xmlns:xl="http://www.w3.org/1999/xlink" 
	xmlns:xml="http://www.w3.org/XML/1998/namespace"
	version="5">
	<glossentry xml:id="unimodal"><glossterm>unimodal</glossterm>
		<glossdef>
			<para>A function with a single local minimum (or maximum) such that
				the local minimum (or maximum) is also the global minimum (or
				maximum).</para>
		</glossdef>
	</glossentry>
</glossary>